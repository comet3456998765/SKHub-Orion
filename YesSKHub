local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local keys = {
    "Chinese",
	"brad",
	"User for SKHub",
}

local function checkKey(key)
    for i, k in ipairs(keys) do
        if key == k then
            return true
        end
    end
    return false
end

OrionLib:MakeNotification({
	Name = "SK Hub Key system",
	Content = "SK Hub is Loading... (8 seg)",
	Image = "rbxassetid://4483345998",
	Time = 5
})
Wait(8)
OrionLib:MakeNotification({
	Name = "SK Hub Key system",
	Content = "SK Hub is Ready! (6 seg)",
	Image = "rbxassetid://4483345998",
	Time = 4
})
Wait(6)
local Window = OrionLib:MakeWindow({Name = "SK Hub (Orion)", HidePremium = false,IntroText = "SK Hub Key system",  SaveConfig = true, ConfigFolder = "OrionTest"})
local Tab = Window:MakeTab({
	Name = "Key system",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Key"
})
Tab:AddParagraph("SKHub V16 (Orion) ©","please insert the key, if you do not have the key contact us on discord or if you are someone close to me use your private key are the first 3 letters or numbers of your name!,  Español: ¡Por favor inserte la clave, si no tienes la clave contacta con nosotros en Discord o si eres alguien cercano ami usa tu Clave privada son las primeras 4 letras o números de tu nombre!")
Tab:AddButton({
	Name = "Enter your key :v",
	Callback = function()
		local key = Tab:AddTextbox({
			Name = "Enter Key",
			Default = "Enter Key Here",
			TextDisappear = true,
			Callback = function(Value)
				if checkKey(Value) then
					loadstring(game:HttpGet("https://raw.githubusercontent.com/comet3456998765/SKHub-Orion/"))()
				else
				    Tab:AddParagraph("invalid key!","")
					Tab:AddParagraph("you want to run the old version (Kavo lib)","")
					Tab:AddButton({
	Name = "Execute Old version",
	Callback = function()
	            loadstring(game:HttpGet("https://raw.githubusercontent.com/comet3456998765/YesSKHUB/main/Main1"))()
  	end    
})
				end
			end	  
		})
  	end    
})

OrionLib:Init()

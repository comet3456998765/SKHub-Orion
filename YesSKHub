local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "SK Hub (Orion) Key system", HidePremium = false,IntroText = "SK Hub Key System",  SaveConfig = true, ConfigFolder = "OrionTest"})
local keys = {} -- Tabla vac√≠a para almacenar las claves generadas

local function generateKey()
    -- Generar una clave aleatoria de 20 caracteres
    local key = ""
    local characters = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789"
    for i = 1, 20 do
        key = key .. characters:sub(math.random(1, #characters), math.random(1, #characters))
    end
    table.insert(keys, key) -- Agregar la clave generada a la tabla de claves
    return key
end

local function checkKey(key)
    for i, k in ipairs(keys) do
        if key == k then
            return true
        end
    end
    return false
end
local Tab = Window:MakeTab({
	Name = "Key system",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
Tab:AddButton({
	Name = "Generate Key",
	Callback = function()
		local generatedKey = generateKey()
		Tab:AddParagraph("Generated Key: " .. generatedKey)
		Tab:AddParagraph("Please give this key to the user.")
        Tab:AddButton({
            Name = "Copy Key",
            Callback = function()
                      setclipboard(generatedKey)
              end    
        })
	end
})
Tab:AddTextbox({
	Name = "Enter Key",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		if checkKey(Value) then
			Tab:AddParagraph("Key is valid! (wait 10 seconds)")
			Wait("10")
            loadstring(game:HttpGet("https://raw.githubusercontent.com/comet3456998765/SKHub-Orion/main/Your%20hub%20men"))()
		else
			Tab:AddParagraph("Invalid key!")
            Tab:AddParagraph("you want to run the old version does not have a key!")
            Tab:AddButton({
                Name = "Run old version",
                Callback = function()
                    loadstring(game:HttpGet("https://raw.githubusercontent.com/comet3456998765/YesSKHUB/main/Main1"))()
                  end    
            })
		end	  
	end
})
local Tab = Window:MakeTab({
	Name = "Changelog",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Section = Tab:AddSection({
	Name = "Info"
})
Tab:AddParagraph("Changelog","(+) Added Changelog (+) Added New Key System 2.0 (+) Added Legends Of Speed script (+) Added Oaklands script")

OrionLib:Init()
